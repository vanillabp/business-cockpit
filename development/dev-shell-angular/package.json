{
  "name": "@vanillabp/bc-dev-shell-angular-module",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/vanillabp/business-cockpit.git"
  },
  "homepage": "https://github.com/vanillabp/business-cockpit",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build dev-shell",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "unpublish:snapshot": "cd dist/dev-shell && npm unpublish --force",
    "publish:snapshot": "cd dist/dev-shell && npm publish --tag snapshot",
    "version:snapshot": "cd dist/dev-shell && npm version ${npm_config_newversion} --no-git-tag-version",
    "publish:release": "cd dist/dev-shell && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@primeng/themes": "^19.1.0",
    "primeng": "^19.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular/cli": "^17.2.2",
    "@angular/compiler-cli": "^17.2.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.3.2"
  }
}
